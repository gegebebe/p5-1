{
  "name": "p5-typescript-template",
  "version": "1.0.0",
  "description": "A template for a p5.js sketch using Typescript ,",
  "scripts": {
    "start": "parcel ./src/index.html --open --port 3000",
    "build": "parcel build ./src/index.html",
    "watch": "parcel watch ./src/index.html",
    "clean": "rm -rf .cache"
  },
  "dependencies": {
    "clmtrackr": "^1.1.2",
    "ml5": "^0.8.1",
    "p5": "^1.4.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.2.1",
    "@types/clmtrackr": "^1.1.1",
    "@types/p5": "^1.3.3",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "sass": "^1.49.0",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "p5js",
    "typescript",
    "sound"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "./src/assets",
        "staticOutDir": "assets"
      }
    ]
  }
}